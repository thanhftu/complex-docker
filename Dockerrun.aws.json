{
    "AWSEBDockerrunVersion":2,
    "containerDefinitions":[
        {
            "name":"client",
            "image":"thanhdhnt/complex-client",
            "hostname":"client",
            "essential":false,
            "memory":128
        },
        {
            "name":"server",
            "image":"thanhdhnt/complex-api",
            "hostname":"api",
            "essential":false,
            "memory":128
        },
        {
            "name":"nginx",
            "image":"thanhdhnt/complex-nginx",
            "hostname":"nginx",
            "essential":true,
            "portMappings":[
                {
                    "hostPort":80,
                    "containerPort":80
                }
            ],
            "links":["client","server"],
            "memory":128
        }
    ]
}